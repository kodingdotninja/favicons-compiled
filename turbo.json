{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": ["yarn.lock"],
  "pipeline": {
    "build": {
      "dependsOn": ["clean", "^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
