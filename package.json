{
  "name": "@kodingdotninja/favicons-compiled",
  "version": "0.0.0",
  "packageManager": "yarn@1.22.18",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --scope=favicons-compiled-src --include-dependencies --no-deps",
    "clean": "turbo run clean",
    "deduplicate": "npx yarn-deduplicate && yarn install",
    "favicons": "yarn workspace favicons-compiled",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "release": "yarn workspace favicons-compiled publish",
    "release:cli": "yarn workspace favicons-compiled-cli publish",
    "test": "turbo run test"
  },
  "dependencies": {
    "@vercel/ncc": "^0",
    "favicons": "7.0.0-beta.4"
  },
  "devDependencies": {
    "husky": "^7",
    "lint-staged": "^12",
    "postinstall-postinstall": "^2",
    "shx": "^0",
    "turbo": "^1"
  },
  "eslintConfig": {
    "extends": [
      "@project/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules"
    ],
    "root": true
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prettier": "@project/prettier-config",
  "private": true
}
